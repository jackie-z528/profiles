# focus window
alt - j : yabai -m window --focus north
alt - l : yabai -m window --focus east
alt - k : yabai -m window --focus south
alt - h : yabai -m window --focus west

# swap window
shift + alt - j : yabai -m window --swap north
shift + alt - l : yabai -m window --swap east
shift + alt - k : yabai -m window --swap south 
shift + alt - h : yabai -m window --swap west

# Switch to first workspace
alt - f : yabai -m space --focus first
# Switch to recent workspace
alt - r : yabai -m space --focus recent
# minimize current window
alt - m : yabai -m window --minimize
alt - z : yabai -m window --space prev
alt - x : yabai -m window --space next

alt - n : yabai -m space --create
shift + alt - d : yabai -m space --destroy
alt - a : yabai -m space --focus prev
alt - s : yabai -m space --focus next
shift + alt - w : yabai -m window --close
